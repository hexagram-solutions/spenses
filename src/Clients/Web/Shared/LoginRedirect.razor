@using System.Web
@inject NavigationManager NavigationManager

@attribute [AllowAnonymous]

@code {

  protected override void OnInitialized()
  {
    var basePath = HttpUtility.UrlEncode(NavigationManager.ToBaseRelativePath(NavigationManager.Uri));

    // NavigationManager.NavigateTo($"/authentication/login?returnUrl={basePath}", true);
    NavigationManager.NavigateTo($"/authentication/login", true);
  }

}
